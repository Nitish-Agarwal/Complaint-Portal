        <activity
            android:name=".Complaint"
            android:label="@string/title_activity_complaint"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Submit_complaint"
            android:label="@string/title_activity_submit_complaint"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Problems"
            android:label="@string/title_activity_problems"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
